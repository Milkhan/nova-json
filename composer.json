{
    "name": "naoray/nova-json",
    "description": "Nova json field to spread a json column throughout multiple fields.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Krishan Koenig",
            "email": "krishan.koenig@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Naoray\\NovaJson\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naoray\\NovaJson\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
